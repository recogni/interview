all: cmain
		./cmain

################################################################################
#
# C env checking.
#
GCC := gcc
GCCFLAGS := -Wall -g


cmain: main.o
		$(GCC) $< -o $@ -lpthread

%.o: %.c
		$(GCC) $(GCCFLAGS) -c $< -o $@

################################################################################
#
# PY env checking.
#

################################################################################
#
# Cleanup.
#
clean:
		rm -f main.o cmain
